{
  "name": "question-asker",
  "version": "0.0.1",
  "main": "src/index.ts",
  "author": "Victor Forissier <dev@victor.to>",
  "license": "MIT",
  "bin": "build/question-asker",
  "dependencies": {
    "cli-progress": "^3.12.0",
    "commander": "^11.0.0",
    "esbuild": "0.15.10",
    "fp-ts": "^2.16.0",
    "io-ts": "^2.2.20",
    "node-fetch": "^2.6.1",
    "polyfact": "^0.1.7",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "8.0.1",
    "koumu": "0.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "prepare": "husky install && koumu setup --copy-into-husky",
    "start": "ts-node src/index.ts",
    "lint": "prettier --check src/ ; eslint src/",
    "lint:fix": "prettier --write src/ ; eslint --fix src/",
    "build": "esbuild src/index.ts --bundle --outfile=build/question-asker.tmp --platform=node && echo -n '#!/usr/bin/env node\n' | cat - build/question-asker.tmp > build/question-asker && rm build/question-asker.tmp"
  }
}
